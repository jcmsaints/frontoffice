<%- contentFor('body') %>

<div class="row ">
  <div
    style="width:100%;
 
margin-top: 5%;
margin-left: 3%;
margin-right: 3%;
margin-bottom: 5%;
"
  >
    <table id="example" class="display" style="width:100%;">
      <thead>
        <tr>
          <% headerList.forEach(function(header) { %>
          <th><%= header.label %></th>

          <% }) %>
        </tr>
      </thead>
      <tbody>
        <% expenseList.forEach(function(expense) { %>
        <tr>
          <% headerList.forEach(function(header) { %> <%if
          (header.id=='imageStr') { %>
          <td>
            <img
              style="
          height: 100px;
      "
              src="<%= expense[header.id] %>"
            />
          </td>
          <% } else{ %>
          <td><%= expense[header.id] %></td>
          <% } %> <% }) %>
        </tr>
        <% }) %>
      </tbody>
    </table>
    <script>
      addDataTable("#example");
    </script>
  </div>
</div>
